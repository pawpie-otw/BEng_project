from typing import Dict, Any

import numpy as np
from functools import partial

def find_by_value(dict_:Dict[Any,Any], value_:Any)->Any:
    """Return key of given value. If return None, not found value in dict.

    Args:
        `dict_` (Dict[Any,Any]): dict to search
        `value_` (Any): value of key you look for

    Returns:
        Any: key or `None` if not found.
    """
    
    for key in dict_:
        if dict_[key] == value_:
            return key
    else:
        raise KeyError(f"No value {value_} in dict. Values: {dict_.values()}")

number_of_lan_mapper = {
        "no_one": 0,
        "one": 1,
        "two": 2,
        "tree_or_more": 3,
        None:None,
        None: np.nan }

def map_languages(lang_num):
    if lang_num in {np.nan, None}:
        return None
    else:
        find_by_value(number_of_lan_mapper, lang_num)
    

print(find_by_value(number_of_lan_mapper ,np.nan))
print(find_by_value(number_of_lan_mapper ,None))